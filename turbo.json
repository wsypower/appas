{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "new-only"
    },
    "stub": {
      "cache": false
    },
    "link": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "publish": {
      "cache": false
    }
  }
}
