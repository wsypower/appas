{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.6",
  "description": "一个初始化模板",
  "author": {
    "name": "wsy"
  },
  "license": "MIT",
  "keywords": [
    "template"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.12 || >=20.9"
  },
  "scripts": {
    "commit": "git cz",
    "bootstrap": "pnpm install",
    "clean": "rimraf node_modules dist",
    "release": "bumpp -r && changelog",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dep:view": "npx taze -r ",
    "dep:update": "npx taze major -wr"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@pubinfo/changelog": "^1.0.3",
    "bumpp": "^9.4.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9.0.0"
      }
    }
  }
}
