{
  "name": "@pubinfo/apaas",
  "type": "module",
  "version": "0.0.12",
  "description": "apaas vite适配插件",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsypower/appas.git",
    "directory": "packages/apaas"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "vite",
    "apaas"
